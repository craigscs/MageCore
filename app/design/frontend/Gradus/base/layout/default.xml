<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="js/core.min.js"/>
        <script src="js/jquery-2.1.0.min.js"/>
        <script src="js/titan.min.js"/>
    </head>
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/vello_logo.png</argument>
                <argument name="logo_img_width" xsi:type="number">225</argument>
                <argument name="logo_img_height" xsi:type="number">80</argument>
            </arguments>
        </referenceBlock>
    </body>
</page>